<?php

/**
 * @file
 * Contains di_nodes_timeline.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function di_nodes_timeline_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the di_nodes_timeline module.
    case 'help.page.di_nodes_timeline':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Timeline and timeline slide content types and fields.') . '</p>';
      return $output;

    default:
  }
}
